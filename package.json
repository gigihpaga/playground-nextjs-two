{
    "name": "playground-nextjs-two",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prepare": "husky install",
        "commit": "git-cz",
        "prepare-commit": "chmod +x .husky/*",
        "ts:check": "tsc --noEmit",
        "format:check": "npx prettier --check \"./src/**/*.{js,cjs,mjs,jsx,ts,tsx,mts,css,scss,sass,md,mdx,yml,yaml,}\"",
        "format:write": "npx prettier --write \"./src/**/*.{js,cjs,mjs,jsx,ts,tsx,mts,css,scss,sass,md,mdx,yml,yaml,}\""
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "next": "14.2.3",
        "react": "^18",
        "react-dom": "^18"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8",
        "eslint-config-next": "14.2.3",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^8.0.0",
        "lint-staged": "^15.2.5",
        "postcss": "^8",
        "prettier": "^3.3.1",
        "shell-quote": "^1.8.1",
        "tailwindcss": "^3.4.1",
        "tsc-files": "^1.1.4",
        "typescript": "^5"
    }
}
