{
    "name": "playground-nextjs-two",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "concurrently \"npm:cli:dev\" \"npm:next:dev\"",
        "cli:dev": "tsx watch cli/index.ts --tsconfig tsconfig.json --clear-screen=true",
        "_cli:dev": "node --require tsx/cjs --watch cli/index.ts cli/index.mts --clear-screen=true",
        "next:dev": "next dev",
        "clean": "rimraf --glob .next",
        "build": "next build",
        "build:docs": "npx contentlayer build",
        "build:sitemap": "next-sitemap",
        "start": "next start",
        "lint": "next lint",
        "postinstall": "prisma generate",
        "postbuild": "next-sitemap",
        "prepare": "husky install",
        "commit": "git-cz",
        "prepare-commit": "chmod +x .husky/*",
        "ts:check": "tsc --noEmit",
        "format:check": "npx prettier --check \"./src/**/*.{js,cjs,mjs,jsx,ts,tsx,mts,css,scss,sass,md,mdx,yml,yaml,}\"",
        "format:check:all": "npx prettier --check \"./**/*.{js,cjs,mjs,jsx,ts,tsx,mts,css,scss,sass,md,mdx,yml,yaml,}\"",
        "format:write": "npx prettier --write \"./src/**/*.{js,cjs,mjs,jsx,ts,tsx,mts,css,scss,sass,md,mdx,yml,yaml,}\"",
        "format:write:all": "npx prettier --write \"./**/*.{js,cjs,mjs,jsx,ts,tsx,mts,css,scss,sass,md,mdx,yml,yaml,}\"",
        "prisma:seed": "prisma db seed",
        "prisma:studio": "npx prisma studio",
        "prisma:generate": "npx prisma generate",
        "prisma:migrate": "npx prisma migrate dev",
        "prisma:migrate:reset": "npx prisma migrate reset",
        "prisma:push": "npx prisma db push",
        "storybook": "storybook dev -p 6006",
        "storybook:build": "storybook build"
    },
    "type": "module",
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "overrides": {
        "next-contentlayer": {
            "next": "$next"
        }
    },
    "prisma": {
        "seed": "tsx prisma/seed/seeder.ts",
        "_seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
        "__seed": "ts-node -r tsconfig-paths/register --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    },
